{
  "ConnectionStrings": {
    "AppDatabase": "{{ DB_CONNECTION_STRING }}",
    "AppDatabase_SA": "{{ SYSADMIN_DB_CONNECTION_STRING }}"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "FrontendEndPoints": "{{ FE_ENDPOINTS }}",


  "Audience": {
    "Secret": "{{ SECRET_KEY_STRING }}",
    "Iss": "{{ ISSUER_STRING }}",
    "Aud": "{{ AUDIENCE_STRING }}",
    "TokenExpiryMinutes": "{{ EXPIRATION_TIME }}"
  },
  
  "HasherSettings": {
    "Salt": "{{ SALT_BASE64_STRING }}"
  }
}
